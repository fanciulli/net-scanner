{
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "start": "node index.mjs",
    "test": "node --experimental-vm-modules --experimental-sqlite ./node_modules/.bin/jest --coverage"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.mjs"
    ],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "testRegex": "tests/.*\\.test\\.mjs",
    "transform": {}
  },
  "dependencies": {
    "@youngkiu/pino-slack-webhook": "^0.1.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "12.1.0",
    "netmask": "2.0.2",
    "pino": "^9.5.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/netmask": "^2.0.5",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0"
  }
}
